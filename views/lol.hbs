<!DOCTYPE html>
<html>
    <head>
        {{> head}}
    </head>
    <body>
        {{> topnav}}
        <div id="content" style="margin-top:75px">
            <div align="center">
                <form action="findMatches" method="get">
                    <h1>Lane: </h1>
                    <select id="lane" name="lane" class="select-css" style="font-family: 'Oxygen'">
                        <option value="0">Top</option>
                        <option value="1">Mid</option>
                        <option value="2">Bottom</option>
                        <option value="3">Support</option>
                        <option value="4">Jungle</option>
                    </select>
                    <br>

                    <h1>Rank: </h1>
                    <select id="rank" name="rank" class="select-css" style="font-family: 'Oxygen'">
                        <option value="0">Iron</option>
                        <option value="1">Bronze</option>
                        <option value="2">Silver</option>
                        <option value="3">Gold</option>
                        <option value="4">Platinum</option>
                        <option value="2">Diamond+</option>
                    </select>
                    <br>
                    <div class="wrap">
                        <div class="btn-6" style="margin-top: 50px" onclick="document.forms[0].submit()">find your party<span></span></div>
                    </div>
                </form>

                <script src="js/button.js"></script>
            </div>
        </div>
    </body>
</html>