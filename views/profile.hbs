<!DOCTYPE html>
<html>
    <head>
        {{> head}}
    </head>
    <body>
        {{> topnav}}
        <div class="wrapper" style="margin-top:150px;">
            <div class="profile-background">
                <div class="filter-black"></div>
            </div>
            <div class="profile-content section-nude">
                <div class="container">
                    <div class="row owner">
                        <div class="col-md-2 col-md-offset-5 col-sm-4 col-sm-offset-4 col-xs-6 col-xs-offset-3 text-center">
                            <div class="avatar">
                                <img src="media/favicon.png" alt="Circle Image"
                                    class="img-circle img-no-padding img-responsive">
                            </div>
                            <div class="name">
                                <h4>{{userD.display_name}}<br /><small>Gamer</small></h4>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6 col-md-offset-3 text-center">
                            <p>Trying to find gamers </p>
                            <br />
                            <button class="btn btn-info"type = "submit">Settings</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </body>
</html>
